pipeline {
    agent any 
    stages {
        stage ("1. fetch current folder path") {
            steps {
                sh "pwd"
                
            }
        }
         stage ("2. Create folder name called praveen") {
            steps {
                sh "mkdir praveen"
		sh "cd praveen"
              
            }
        }
        stage ("3. create file under praveen folder") {
            steps {
                sh "touch file2.txt"
                
            }
        }
        
    }
}
